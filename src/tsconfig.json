{
  "exclude": ["sw.ts", "hljs.ts"],
  "compilerOptions": {
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "types": ["svelte"],
    "useUnknownInCatchVariables": false
  }
}
